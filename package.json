{
  "name": "reg-cli",
  "version": "0.17.0",
  "description": "",
  "main": "./dist/index.js",
  "start": "node ./dist/cli.js",
  "scripts": {
    "watch": "babel src --watch -d dist",
    "build": "npm run build:cli && npm run build:report",
    "build:cli": "babel src -d dist",
    "build:report": "sh ./scripts/build-ui.sh v0.2.0",
    "dev:report": "npm run copy:ximgdiff && webpack-dev-server --hot --inline --open",
    "flow": "flow",
    "copy:ximgdiff": "copyfiles -u 3 node_modules/x-img-diff-js/build/cv-wasm_browser.* report/assets",
    "prepublishOnly": "npm run build",
    "reg": "node dist/cli.js ./sample/actual ./sample/expected ./sample/diff -I -R ./sample/index.html -T 0.01 -X client",
    "reg:from": "node dist/cli.js -F ./sample/reg.json -R ./sample/index.html",
    "screenshot": "node test/screenshot.js",
    "test:cli": "chmod +x dist/cli.js && ava test/cli.test.js",
    "test:screenshot": "npm run build:report && npm run reg && npm run screenshot",
    "test": "cross-env NODE_ENV=\"debug\"  npm run build:report && npm run build && npm run test:cli && npm run test:screenshot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reg-viz/reg-cli/"
  },
  "author": "bokuweb",
  "license": "MIT",
  "resolutions": {
    "**/set-value": "2.0.1",
    "**/randomatic": "3.1.1",
    "**/deep-extend": "0.6.0",
    "**/cryptiles": "4.1.3",
    "**/hoek": "6.1.3",
    "**/dot-prop": "^6.0.0",
    "**/node-forge": "^0.10.0",
    "**/serialize-javascript": "^5.0.0"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "chalk": "4.1.0",
    "cli-spinner": "0.2.10",
    "cross-spawn": "7.0.3",
    "del": "6.0.0",
    "glob": "7.1.6",
    "img-diff-js": "0.5.1",
    "lodash": "4.17.21",
    "make-dir": "3.1.0",
    "md5-file": "4.0.0",
    "meow": "3.7.0",
    "mustache": "4.1.0",
    "x-img-diff-js": "0.3.5"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-es2015-block-scoping": "6.26.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copyfiles": "2.1.1",
    "cross-env": "7.0.3",
    "finalhandler": "1.1.2",
    "flow-bin": "0.77.0",
    "flow-typed": "3.2.1",
    "husky": "4.3.0",
    "puppeteer": "1.20.0",
    "reg-keygen-git-hash-plugin": "0.10.16",
    "reg-notify-github-plugin": "0.10.10",
    "reg-publish-s3-plugin": "0.10.10",
    "reg-suit": "0.10.10",
    "rimraf": "3.0.2",
    "serve-static": "1.14.1",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1"
  },
  "files": [
    "dist",
    "template",
    "report/ui/dist/report.js",
    "report/ui/dist/worker.js",
    "report/assets/favicon_failure.png",
    "report/assets/favicon_success.png",
    "report/sample"
  ],
  "bin": {
    "reg-cli": "./dist/cli.js"
  }
}
